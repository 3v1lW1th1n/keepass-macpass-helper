{
  "manifest_version": 2,
  "version": "0.3.2",
  "name": "KeePassHelper Password Manager",
  "description": "Helper extension for KeePass to find and insert logins and passwords without copying them",
  "permissions": [
    "storage",
    "activeTab",
    "<all_urls>",
    "contextMenus",
    "clipboardWrite",
    "notifications"
  ],
  "optional_permissions": [
    "webNavigation"
  ],
  "background": {
    "persistent": false,
    "scripts":[
      "sjcl.js",
      "keepass.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "64": "data/icons/64.png"
    }
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png",
    "256": "data/icons/256.png",
    "512": "data/icons/512.png"
  },
  "web_accessible_resources": [
    "data/save/index.html",
    "data/cmd/index.html",
    "data/cmd/loading.svg"
  ],
  "homepage_url": "http://add0n.com/keepass-helper.html",
  "options_ui": {
    "page": "data/options/index.html",
    "chrome_style": true
  },
  "applications": {
    "gecko": {
      "id": "{69ef9498-0139-43e4-97b8-942982ac9158}",
      "strict_min_version": "52.0"
    }
  }
}
