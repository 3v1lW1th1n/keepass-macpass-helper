{
  "manifest_version":2,
  "version":"0.1.2",
  "name":"KeePassHelper",
  "description":"Helper extension to find and insert logins and passwords without copying them",
  "permissions":[
    "storage",
    "activeTab",
    "<all_urls>",
    "contextMenus"
  ],
  "background":{
    "persistent": false,
    "scripts":[
      "sjcl.js",
      "keepass.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "64": "data/icons/64.png"
    }
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png",
    "256": "data/icons/256.png",
    "512": "data/icons/512.png"
  },
  "web_accessible_resources": [
    "data/cmd/*"
  ],
  "homepage_url": "http://add0n.com/keepass-helper.html"
}
